@model IEnumerable<MVCExample.Models.tbl_upload>
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
    <div> 
        @using (Html.BeginForm("Index","FileUpload",FormMethod.Post,new {enctype="multipart/form-data"}))
        {
        <p>Enter File to Insert Record</p>
        <p><input type="file" name="file" /></p>
        <p><input type="submit" name="btnsubmit" value="Upload" /></p>
            

        }
        <center>@ViewBag.data</center>
        <br />
        @foreach(var item in Model)
        {
            <img src="~/upload/@Html.DisplayFor(a=>item.imgname)" width="100" height="100" />

        }
    </div>
</body>
</html>
